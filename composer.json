{
    "name": "contao/manager-bundle",
    "type": "symfony-bundle",
    "description": "Provides the Contao Managed Edition",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "contao/core-bundle": "self.version",
        "contao/installation-bundle": "self.version",
        "contao/maintenance-bundle-deprecated": "^2.1.5",
        "contao/manager-plugin": "^2.4",
        "doctrine/dbal": "^2.11",
        "doctrine/doctrine-bundle": "^1.8 || ^2.0",
        "friendsofsymfony/http-cache": "^2.6",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "nelmio/cors-bundle": "^1.5.3 || ^2.0.1",
        "nelmio/security-bundle": "^2.2",
        "ocramius/proxy-manager": "^2.1",
        "symfony/cache": "4.4.* || 5.2.*",
        "symfony/config": "4.4.* || 5.2.*",
        "symfony/console": "4.4.* || 5.2.*",
        "symfony/debug": "4.4.* || 5.2.*",
        "symfony/debug-bundle": "4.4.* || 5.2.*",
        "symfony/dependency-injection": "4.4.* || 5.2.*",
        "symfony/deprecation-contracts": "^2.1",
        "symfony/dotenv": "4.4.* || 5.2.*",
        "symfony/doctrine-bridge": "4.4.* || 5.2.*",
        "symfony/expression-language": "4.4.* || 5.2.*",
        "symfony/filesystem": "4.4.* || 5.2.*",
        "symfony/finder": "4.4.* || 5.2.*",
        "symfony/framework-bundle": "4.4.* || 5.2.*",
        "symfony/http-client": "4.4.* || 5.2.*",
        "symfony/http-foundation": "4.4.* || 5.2.*",
        "symfony/http-kernel": "4.4.* || 5.2.*",
        "symfony/mailer": "4.4.* || 5.2.*",
        "symfony/monolog-bridge": "4.4.* || 5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.4.* || 5.2.*",
        "symfony/proxy-manager-bridge": "4.4.* || 5.2.*",
        "symfony/routing": "4.4.* || 5.2.*",
        "symfony/security-bundle": "4.4.* || 5.2.*",
        "symfony/stopwatch": "4.4.* || 5.2.*",
        "symfony/twig-bundle": "4.4.* || 5.2.*",
        "symfony/web-profiler-bundle": "4.4.* || 5.2.*",
        "symfony/yaml": "4.4.* || 5.2.*",
        "toflar/psr6-symfony-http-cache-store": "^2.1 || ^3.0",
        "webmozart/path-util": "^2.2"
    },
    "require-dev": {
        "composer/composer": "^1.0 || ^2.0",
        "contao/test-case": "^4.3",
        "phpunit/phpunit": "^8.5",
        "symfony/phpunit-bridge": "^5.2"
    },
    "conflict": {
        "symfony/config": "<4.4.2",
        "symfony/security-bundle": "4.4.* <4.4.5"
    },
    "suggest": {
        "contao/tcpdf-bundle": "To export articles as PDF files"
    },
    "extra": {
        "contao-manager-plugin": "Contao\\ManagerBundle\\ContaoManager\\Plugin",
        "symfony": {
            "require": "4.4.* || ^5.2"
        }
    },
    "autoload": {
        "psr-4": {
            "Contao\\ManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ManagerBundle\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/contao-console",
        "bin/contao-setup"
    ],
    "support": {
        "issues": "https://github.com/contao/contao/issues",
        "forum": "https://community.contao.org",
        "source": "https://github.com/contao/manager-bundle",
        "docs": "https://docs.contao.org"
    }
}
