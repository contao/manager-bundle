{
    "name": "contao/manager-bundle",
    "type": "symfony-bundle",
    "description": "Contao 4 manager bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Andreas Schempp",
            "homepage": "https://github.com/aschempp"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "^3.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/swiftmailer-bundle": "^2.3",
        "sensio/framework-extra-bundle": "^3.0.2",
        "doctrine/doctrine-bundle": "^1.6",
        "friendsofsymfony/http-cache": "^2.0",
        "lexik/maintenance-bundle": "^2.0",
        "nelmio/security-bundle": "^2.2",
        "terminal42/header-replay-bundle": "^1.0",
        "contao/manager-plugin": "^2.1",
        "contao/core-bundle": "~4.4.0@dev",
        "contao/installation-bundle": "~4.4.0@dev"
    },
    "require-dev": {
        "composer/composer": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.0",
        "satooshi/php-coveralls": "^1.0",
        "symfony/phpunit-bridge": "^3.2"
    },
    "conflict": {
        "contao/calendar-bundle": "<4.3.4",
        "contao/comments-bundle": "<4.3.4",
        "contao/faq-bundle": "<4.3.4",
        "contao/listing-bundle": "<4.3.4",
        "contao/news-bundle": "<4.3.4",
        "contao/newsletter-bundle": "<4.3.4"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ManagerBundle\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/contao-console"
    ],
    "extra": {
        "branch-alias": {
            "dev-release/4.4.0": "4.4.x-dev"
        },
        "contao-manager-plugin": "Contao\\ManagerBundle\\ContaoManager\\Plugin"
    }
}
